{
  "name": "vvm-logical",
  "version": "0.0.1",
  "description": "Visualization of Virtual Machines for Logical Languages",
  "private": true,
  "homepage": ".",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --outDir build",
    "build:gh": "tsc && vite build --outDir build --base /vvm-logic/",
    "serve": "vite preview --outDir build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "cytoscape": "^3.25.0",
    "cytoscape-fcose": "^2.2.0",
    "immutable": "^4.3.0",
    "jison": "^0.4.18",
    "parser-generator": "^0.2.0",
    "react": "^18.2.0",
    "react-cytoscapejs": "^2.0.0",
    "react-dom": "^18.2.0",
    "react-tooltip": "^4.5.1",
    "typescript": "^5.1.3",
    "usehooks-ts": "^2.9.1",
    "web-vitals": "^3.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cytoscape-fcose": "^2.2.0",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.14",
    "@types/react-cytoscapejs": "^1.2.2",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.32.2",
    "eslint": "^8.43.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^22.1.0",
    "sass": "^1.63.6",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
