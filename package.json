{
  "name": "vvm-logical",
  "version": "0.0.1",
  "description": "Visualization of Virtual Machines for Logical Languages",
  "private": true,
  "homepage": ".",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --outDir build",
    "build:gh": "tsc && vite build --outDir build --base /vvm-logic/",
    "serve": "vite preview --outDir build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "cytoscape": "^3.28.1",
    "cytoscape-fcose": "^2.2.0",
    "immutable": "^4.3.4",
    "jison": "^0.4.18",
    "parser-generator": "^0.2.0",
    "react": "^18.2.0",
    "react-cytoscapejs": "^2.0.0",
    "react-dom": "^18.2.0",
    "react-tooltip": "^4.5.1",
    "typescript": "^5.3.3",
    "usehooks-ts": "^2.9.2",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/cytoscape-fcose": "^2.2.4",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.48",
    "@types/react-cytoscapejs": "^1.2.5",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-c8": "^0.33.0",
    "eslint": "^8.56.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^24.0.0",
    "sass": "^1.70.0",
    "vite": "^5.0.12",
    "vitest": "^0.34.6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
